export default class BaseControl {
  public element: HTMLElement;
  protected uri: string;
  constructor(element:HTMLElement, uri:string){
    this.element = element;
    this.uri = uri;
  }

  public render(uri?:string):WinJS.Promise<HTMLElement> {
    return WinJS.UI.Fragments.render(uri || this.uri, this.element)
  }
}